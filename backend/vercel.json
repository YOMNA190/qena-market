{
  "version": 2,
  "name": "qena-market-backend",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/schema.prisma", "src/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run build && npx prisma generate && npx prisma migrate deploy",
  "installCommand": "npm install"
}
